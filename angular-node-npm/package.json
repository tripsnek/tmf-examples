{
  "name": "angular-node-npm",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "dev:server": "npm run dev -w @tmf-example/server",
    "dev:client": "npm run start -w @tmf-example/client",
    "build:data-model": "npm run build -w @tmf-example/data-model",
    "build:server": "npm run build -w @tmf-example/server",
    "build:client": "npm run build -w @tmf-example/client",
    "clean": "npm run clean --workspaces --if-present"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.6.0"
  },
  "dependencies": {
    "@tripsnek/tmf": "^1.1.22"
  }
}
